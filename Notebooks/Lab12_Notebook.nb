Cell[TextData[{
 "Consider an Alternating Renewal Process N(t) with  cycle ",
 Cell[BoxData[
  SubscriptBox["X", "n"]]],
 "= ",
 Cell[BoxData[
  SubscriptBox["Y", "n"]]],
 "+ ",
 Cell[BoxData[
  SubscriptBox["Z", "n"]]],
 ", where ",
 Cell[BoxData[
  SubscriptBox["Z", "n"]]],
 "~U[1,3] with E",
 Cell[BoxData[
  SubscriptBox["Z", "n"]]],
 "=2, ",
 Cell[BoxData[
  SubscriptBox["Y", "n"]]],
 "~exponential ~\[Lambda] =",
 Cell[BoxData[
  FractionBox["1", "2"]]],
 "  with E",
 Cell[BoxData[
  SubscriptBox["Y", "n"]]],
 "=",
 Cell[BoxData[
  FractionBox["1", "\[Lambda]"]]],
 "= 2.  Here ",
 Cell[BoxData[
  SubscriptBox["Z", "n"]]],
 "=\"on\" = service and ",
 Cell[BoxData[
  SubscriptBox["Y", "n"]]],
 " = \"off\" = waiting time for a customer (which arrives according to \
Poisson ~\[Lambda], and quits whenever the servicing person is busy \nwith a \
customer).  Then the fraction of   \"on\" time satisfies"
}], "Text",
 CellChangeTimes->{
  3.732372289882199*^9, {3.7323723367145996`*^9, 3.7323723418471313`*^9}}],

Cell[TextData[Cell[BoxData[
 RowBox[{
  RowBox[{
   UnderscriptBox["lim", 
    RowBox[{"t", "\[Rule]", "\[Infinity]"}]], 
   FractionBox[
    RowBox[{" ", 
     RowBox[{"E", "[", 
      RowBox[{"\"\<on\>\"", " ", "time", " ", 
       RowBox[{"in", " ", "[", 
        RowBox[{"0", ",", "t"}], "]"}]}], "]"}]}], "t"]}], "=", 
  RowBox[{
   FractionBox[
    RowBox[{"E", " ", "[", "on", "]"}], 
    RowBox[{"E", "[", 
     RowBox[{"time", " ", "of", " ", "the", " ", "cycle"}], "]"}]], "=", 
   RowBox[{
    FractionBox[
     RowBox[{"E", "[", "on", "]"}], 
     RowBox[{"E", "[", 
      RowBox[{"on", " ", "+", "off"}], "]"}]], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"E", "[", "on", "]"}], 
      RowBox[{
       RowBox[{"E", "[", "on", " ", "]"}], "+", " ", 
       RowBox[{"E", "[", "off", "]"}]}]], "=", 
     RowBox[{
      FractionBox[
       RowBox[{"E", "[", 
        SubscriptBox["Z", "1"], "]"}], 
       RowBox[{
        SubscriptBox["EZ", "1"], "+", " ", 
        SubscriptBox["EY", "1"]}]], "=", 
      FractionBox["1", "2"]}]}]}]}]}]]]], "Text",
 CellChangeTimes->{{3.699280586729784*^9, 3.699280657007784*^9}}],

Cell["Below is a simulation of  n  samples on the interval [0,t]", "Text"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Z", ":=", 
    RowBox[{"Random", "[", 
     RowBox[{"Real", ",", 
      RowBox[{"{", 
       RowBox[{"1", ",", "3"}], "}"}]}], "]"}]}], ";"}], 
  " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Y", ":=", 
   RowBox[{"Random", "[", 
    RowBox[{"ExponentialDistribution", "[", 
     FractionBox["1", "2"], "]"}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"t", "=", "100"}], ";", 
  RowBox[{"n", "=", "1000"}], ";", " ", 
  RowBox[{"Ont", "=", 
   RowBox[{"{", "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Do", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"TotOn", "=", "0"}], ";", "\n", "      ", 
     RowBox[{"u", "=", "0"}], ";", 
     RowBox[{"on", "=", "Z"}], ";", 
     RowBox[{"off", "=", "Y"}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"u", "+", "on", "+", "off"}], "<", "t"}], ",", " ", 
       RowBox[{
        RowBox[{"u", "=", 
         RowBox[{"u", "+", "on", "+", "off"}]}], ";", " ", 
        RowBox[{"TotOn", "=", 
         RowBox[{"TotOn", "+", "on"}]}], ";", "\[IndentingNewLine]", 
        "         ", 
        RowBox[{"on", "=", "Z"}], ";", 
        RowBox[{"off", "=", "Y"}]}]}], "\[IndentingNewLine]", "         ", 
      "]"}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"on", "<", 
        RowBox[{"t", "-", "u"}]}], ",", " ", 
       RowBox[{"TotOn", "=", 
        RowBox[{"TotOn", "+", "on"}]}], ",", 
       RowBox[{"TotOn", "=", 
        RowBox[{"TotOn", "+", "t", "-", "u"}]}]}], "]"}], ";", 
     RowBox[{"AppendTo", "[", 
      RowBox[{"Ont", ",", 
       RowBox[{"TotOn", "/", "t"}]}], "]"}]}], ",", "\[IndentingNewLine]", 
    RowBox[{"{", "n", "}"}]}], "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Mean", "[", "Ont", "]"}], "//", "N"}], "     "}]}], "Input"],

