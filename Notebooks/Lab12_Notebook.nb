   RowBox[{"{", "}"}]}], ";", 
  RowBox[{"TotReward", "=", "0"}], ";", 
  RowBox[{"s", "=", "0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"r", "=", 
    RowBox[{
     RowBox[{"Random", "[", 
      RowBox[{"Real", ",", 
       RowBox[{"{", 
        RowBox[{"0", ",", "2"}], "}"}]}], "]"}], "+", 
     RowBox[{"Random", "[", 
      RowBox[{"ExponentialDistribution", "[", 
       FractionBox["1", "2"], "]"}], "]"}]}]}], ";", " ", 
   RowBox[{"s", "=", 
    RowBox[{"s", "+", "r"}]}], ";"}], " "}], "\[IndentingNewLine]", 
 RowBox[{"      ", 
  RowBox[{
   RowBox[{"While", "[", 
    RowBox[{
     RowBox[{"s", "<", "t"}], ",", "\[IndentingNewLine]", "              ", 
     RowBox[{
      RowBox[{"Reward", "=", 
       RowBox[{"Random", "[", 
        RowBox[{"Integer", ",", 
         RowBox[{"{", 
          RowBox[{"2", ",", "10"}], "}"}]}], "]"}]}], ";", 
      RowBox[{"AppendTo", "[", 
       RowBox[{"RenewalReward", ",", 
        RowBox[{"{", 
         RowBox[{"s", ",", "Reward"}], "}"}]}], "]"}], ";", " ", 
      "\[IndentingNewLine]", "              ", 
      RowBox[{"TotReward", "=", 
       RowBox[{"TotReward", "+", "Reward"}]}], " ", ";", " ", 
      "\[IndentingNewLine]", "              ", 
      RowBox[{"r", "=", 
       RowBox[{
        RowBox[{"Random", "[", 
