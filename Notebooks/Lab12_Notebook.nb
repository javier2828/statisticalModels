Cell[TextData[{
 "Consider a renewal process N(t) in which the life of an element ",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["X", "i"], " "}]],ExpressionUUID->
  "37d38346-5433-45cf-8258-5274fdbf0293"],
 "~ X~ U[0,1], with  \[Mu]  =",
 Cell[BoxData[
  FractionBox["1", "2"]],ExpressionUUID->
  "baa916a3-f032-49f0-958f-a02650467c03"],
 ", ",
 Cell[BoxData[
  SuperscriptBox["\[Sigma]", "2"]],ExpressionUUID->
  "8d6984c2-1928-4bbf-9bcb-4128beb9080a"],
 " =",
 Cell[BoxData[
  FractionBox["1", "12"]],ExpressionUUID->
  "1138eacc-5e1a-4642-922e-3059cbef23cc"],
 ".  \nThen the renewal function  m(t) = E N(t)   satisfies \n\n",
 Cell[BoxData[
  RowBox[{"                       ", 
   RowBox[{
    RowBox[{
     UnderscriptBox["lim", 
      RowBox[{"t", "\[Rule]", "\[Infinity]"}]], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"m", 
        RowBox[{"(", "t", ")"}]}], " ", "-", " ", 
       FractionBox["t", "\[Mu]"]}], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{
         SuperscriptBox["\[Sigma]", "2"], "-", "  ", 
         SuperscriptBox["\[Mu]", "2"]}], 
        RowBox[{"2", 
         SuperscriptBox["\[Mu]", "2"]}]], "        ", 
       "\[DoubleLongRightArrow]", "       ", "m"}], 
      RowBox[{"(", "t", ")"}]}], "\[TildeTilde]", " ", 
     RowBox[{
      FractionBox["t", "\[Mu]"], "+", 
      RowBox[{
       FractionBox[
        RowBox[{
         SuperscriptBox["\[Sigma]", "2"], "-", "  ", 
         SuperscriptBox["\[Mu]", "2"]}], 
        RowBox[{"2", 
         SuperscriptBox["\[Mu]", "2"]}]], "       ", "for", " ", "large", "  ",
        "t"}]}]}]}]}]],ExpressionUUID->"ed30d472-c7fe-48cf-8c62-af323bb1406b"]
}], "Text",
 CellChangeTimes->{{3.699280188664584*^9, 3.6992802063549843`*^9}, {
  3.7323698251201963`*^9, 
  3.732369829597511*^9}},ExpressionUUID->"b2a860d6-f6bc-435f-91f5-\
43169ff82c84"],

