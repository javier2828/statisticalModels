Cell[TextData[{
 "Below  n = 10  samples of  N(t),  ",
 Cell[BoxData[
  SuperscriptBox["N", "i"]]],
 "(t)  for t \[Element] {0, .05, .1, . . . , 5} are simulated \nand the \
sample average ",
 Cell[BoxData[
  RowBox[{" ", 
   RowBox[{
    OverscriptBox[
     RowBox[{"m", 
      RowBox[{"(", "t", ")"}]}], "_"], " ", "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          SuperscriptBox["N", "1"], 
          RowBox[{"(", "t", ")"}]}], "+"}], "..."}], "+", 
       RowBox[{
        SuperscriptBox["N", "n"], 
        RowBox[{"(", "t", ")"}]}]}], "n"], "\[TildeTilde]", 
     RowBox[{"m", 
      RowBox[{"(", "t", ")"}]}]}]}]}]]],
 " is evaluated"
}], "Text",
 CellChangeTimes->{{3.732372064300415*^9, 3.732372070712179*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"n", "=", "10"}], ";", 
  RowBox[{
   OverscriptBox["m", "_"], "=", 
   RowBox[{"{", "}"}]}], ";", " ", 
  RowBox[{"t", "=", "5"}], ";", " ", 
  RowBox[{"dt", "=", ".05"}], ";", 
  RowBox[{"p", "=", 
   FractionBox["t", "dt"]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Do", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"mt", "=", 
     RowBox[{"{", "}"}]}], ";", "   ", "\n", "    ", 
    RowBox[{"Do", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Numt", "=", 
        RowBox[{"{", "}"}]}], ";", 
       RowBox[{"s", "=", "0"}], ";", " ", 
       RowBox[{"s", "=", 
        RowBox[{"s", "+", 
         RowBox[{"Random", "[", "]"}]}]}], ";", "\[IndentingNewLine]", 
       "     ", 
       RowBox[{"While", "[", 
        RowBox[{
         RowBox[{"s", "<", 
          RowBox[{"i", "*", ".05"}]}], ",", 
         RowBox[{
          RowBox[{"AppendTo", "[", 
           RowBox[{"Numt", ",", "s"}], "]"}], ";", 
          RowBox[{"s", "=", 
           RowBox[{"s", "+", 
            RowBox[{"Random", "[", "]"}]}]}]}]}], "]"}], ";", 
       "\[IndentingNewLine]", "                  ", 
       RowBox[{"AppendTo", "[", 
        RowBox[{"mt", ",", 
         RowBox[{"Length", "[", "Numt", "]"}]}], "]"}]}], ",", 
      "\[IndentingNewLine]", "    ", 
      RowBox[{"{", "n", "}"}]}], "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"AppendTo", "[", 
     RowBox[{
      OverscriptBox["m", "_"], ",", 
      RowBox[{"Mean", "[", "mt", "]"}]}], "]"}]}], ",", "\[IndentingNewLine]", 
   RowBox[{"{", 
    RowBox[{"i", ",", "0", ",", "p"}], "}"}]}], "]"}]}], "Input"],

