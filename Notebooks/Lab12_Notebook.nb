Cell[TextData[{
 "Consider a Renewal-Reward Process N(t) with cycle ",
 Cell[BoxData[
  SubscriptBox["X", "n"]]],
 "= ",
 Cell[BoxData[
  SubscriptBox["Y", "n"]]],
 "+ ",
 Cell[BoxData[
  SubscriptBox["Z", "n"]]],
 ", where ",
 Cell[BoxData[
  SubscriptBox["Y", "n"]]],
 "~U[0,2] with E",
 Cell[BoxData[
  SubscriptBox["Y", "n"]]],
 "=1, \n",
 Cell[BoxData[
  SubscriptBox["Z", "n"]]],
 "~exponential ~\[Lambda] = ",
 Cell[BoxData[
  FractionBox["1", "2"]]],
 " with E",
 Cell[BoxData[
  SubscriptBox["Z", "n"]]],
 "=",
 Cell[BoxData[
  FractionBox["1", "\[Lambda]"]]],
 "= 2 and reward ",
 Cell[BoxData[
  SubscriptBox["R", "n"]]],
 "~U{2,3,...,10} with E",
 Cell[BoxData[
  SubscriptBox["R", "n"]]],
 "= 6.  \nThen the reward ",
 Cell[BoxData[
  SubscriptBox["R", "t"]]],
 "=",
 Cell[BoxData[
  RowBox[{
   UnderoverscriptBox["\[Sum]", 
    RowBox[{"n", "=", "1"}], 
    RowBox[{"N", 
     RowBox[{"(", "t", ")"}]}]], 
   SubscriptBox["R", "n"]}]]],
 " for the renewal process corresponding to {",
 Cell[BoxData[
  SubscriptBox["X", "n"]]],
 "} satisfies \n\n",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    UnderscriptBox["lim", 
     RowBox[{"t", "\[Rule]", "\[Infinity]"}]], 
    FractionBox[
     SubscriptBox["R", "t"], "t"]}], "=", 
   RowBox[{
    FractionBox[
     RowBox[{"E", " ", "[", 
      RowBox[{"reward", " ", "in", " ", "the", " ", "cycle"}], "]"}], 
     RowBox[{"E", "[", 
      RowBox[{"time", " ", "of", " ", "the", " ", "cycle"}], "]"}]], "=", 
    RowBox[{
     FractionBox[
      SubscriptBox["ER", "1"], 
      RowBox[{"E", "[", 
       RowBox[{
        SubscriptBox["Y", "1"], "+", 
        SubscriptBox["Z", "1"]}], "]"}]], "=", 
     RowBox[{
      FractionBox[
       SubscriptBox["ER", "1"], 
       RowBox[{
        SubscriptBox["EY", "1"], "+", " ", 
        SubscriptBox["EZ", "1"]}]], "=", "2"}]}]}]}]]]
}], "Text",
 CellChangeTimes->{{3.732372010861039*^9, 3.732372041180622*^9}, {
  3.732372224875332*^9, 3.732372231864311*^9}}],

