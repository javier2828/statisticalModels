Cell["Generate a sample of size n for Poisson Process on [0, t]", "Text",ExpressionUUID->"9b3ea64d-c364-47ce-9019-cfa004b32ae2"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Clear", "[", 
    RowBox[{
    "a", ",", "s", ",", "t", ",", "r", ",", "\[Lambda]", ",", "m", ",", "n"}],
     "]"}], ";", "  ", 
   RowBox[{"n", "=", "1000"}], ";", 
   RowBox[{"\[Lambda]", "=", "2"}], ";", 
   RowBox[{"t", "=", "10"}], ";", 
   RowBox[{"m", "=", 
    RowBox[{"{", "}"}]}], ";"}], "   "}], "\n", 
 RowBox[{"Do", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"a", "=", "0"}], ";", 
    RowBox[{"s", "=", "0"}], ";", 
    RowBox[{"Label", "[", "start", "]"}], ";", 
    RowBox[{"r", "=", 
     RowBox[{"Random", "[", 
      RowBox[{"ExponentialDistribution", "[", "\[Lambda]", "]"}], "]"}]}], 
    ";", " ", 
    RowBox[{"s", "=", 
     RowBox[{"s", "+", "r"}]}], ";", "  ", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"s", "\[LessEqual]", "t"}], ",", 
      RowBox[{
       RowBox[{"a", "=", 
        RowBox[{"a", "+", "1"}]}], ";", 
       RowBox[{"Goto", "[", "start", "]"}]}]}], "]"}], ";", 
    RowBox[{"AppendTo", "[", 
     RowBox[{"m", ",", "a"}], "]"}]}], ",", 
   RowBox[{"{", "n", "}"}]}], "]"}]}], "Input",ExpressionUUID->"2529acfe-1ac8-\
4703-8b90-93db62729276"],

