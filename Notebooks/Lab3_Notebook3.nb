  3.56996146646345*^9}},ExpressionUUID->"d260d3c7-d959-4cda-9219-\
e5b13523f210"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{
   "a", ",", "s", ",", "r", ",", "t", ",", "u", ",", "f", ",", "\[Lambda]", 
    ",", "Num"}], "]"}], "  "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"t", "=", "10"}], ";", 
   RowBox[{"\[Lambda]", "=", "1"}], ";", 
   RowBox[{"s", "=", 
    RowBox[{"{", "}"}]}], ";", 
   RowBox[{"a", "=", "0"}], ";", 
   RowBox[{"Label", "[", "start", "]"}], ";", 
   RowBox[{"r", "=", 
    RowBox[{"Random", "[", 
     RowBox[{"ExponentialDistribution", "[", "\[Lambda]", "]"}], "]"}]}], ";",
    " ", 
   RowBox[{"a", "=", 
    RowBox[{"a", "+", "r"}]}], ";", "  ", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"a", "\[LessEqual]", "t"}], ",", 
     RowBox[{
      RowBox[{"AppendTo", "[", 
       RowBox[{"s", ",", "a"}], "]"}], ";", 
      RowBox[{"Goto", "[", "start", "]"}]}]}], "]"}], ";"}], 
  "                                                   "}]}], "Input",
