  RowBox[{
   RowBox[{"s", "[", "1", "]"}], "=", "1"}], ";", 
  RowBox[{
   RowBox[{"s", "[", "2", "]"}], "=", "2.6"}], ";", 
  RowBox[{
   RowBox[{"s", "[", "3", "]"}], "=", "5.1"}], ";", " ", 
  RowBox[{
   RowBox[{"A", "[", "1", "]"}], "=", "1"}], ";", 
  RowBox[{
   RowBox[{"A", "[", "2", "]"}], "=", "1.5"}], ";", 
