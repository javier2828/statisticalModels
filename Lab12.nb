Cell[TextData[{
 "    3.   A lifetime of an element has WeibullDistribution[3,6], replacement \
time is Uniform[0,1] and replacement cost ",
 Cell[BoxData[
  SubscriptBox["R", "i"]],ExpressionUUID->
  "bc64a070-734b-4293-a0c4-2e386b79c8ae"],
 " equals \n         twice the replacement time.  Consider a corresponding  \
Renewal - Reward Process  N(t) with  R(t) = ",
 Cell[BoxData[
  RowBox[{
   UnderoverscriptBox["\[Sum]", 
    RowBox[{"i", "=", "1"}], 
    RowBox[{"N", 
     RowBox[{"(", "t", ")"}]}]], 
   SubscriptBox["R", "i"]}]],ExpressionUUID->
  "618719e1-91e9-407f-bcab-6028b8be04d2"],
 ".\n         \n          (a)  Simulate n =1000  samples of ",
 Cell[BoxData[
  FractionBox[
   RowBox[{"R", 
    RowBox[{"(", "t", ")"}]}], "t"]],ExpressionUUID->
  "177166a5-7b02-4d25-a51f-5896b390ce5a"],
 " for t = 100, 200, 500 and compare to ",
 Cell[BoxData[
  SubscriptBox["lim", 
   RowBox[{"t", "\[Rule]", "\[Infinity]"}]]],ExpressionUUID->
  "6dd4570d-97dc-4d41-9c27-2b0316ca03db"],
 Cell[BoxData[
  FractionBox[
   RowBox[{"R", 
    RowBox[{"(", "t", ")"}]}], "t"]],ExpressionUUID->
  "0081aead-52fe-47ea-848a-89d5e014e92e"],
 "\n          (b)  Graph a typical sample of  R(t)  for t =100"
}], "Text",
 CellChangeTimes->{{3.764433018188501*^9, 3.764433023316864*^9}, 
   3.7644345437382803`*^9, {3.764435367841195*^9, 
   3.7644353790205812`*^9}},ExpressionUUID->"13b7272d-5b64-427c-a431-\
adb19217f9c8"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"t", "=", "100"}], ";", 
  RowBox[{"Rt", "=", 
   RowBox[{"{", "}"}]}], ";", 
  RowBox[{"n", "=", "1000"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Do", "[", " ", 
   RowBox[{
    RowBox[{
     RowBox[{"TotReward", "=", "0"}], ";", 
     RowBox[{"s", "=", "0"}], ";", "\[IndentingNewLine]", "   ", 
     RowBox[{"r", "=", 
      RowBox[{
       RowBox[{"RandomReal", "[", "]"}], "+", 
