      RowBox[{"P", "[", "t", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"For", "[", 
    RowBox[{
     RowBox[{"i", "=", "1"}], ",", 
     RowBox[{"i", "\[LessEqual]", "8"}], ",", 
     RowBox[{"i", "=", 
      RowBox[{"i", "*", "2"}]}], ",", 
     RowBox[{"Print", "[", 
      RowBox[{
       RowBox[{"p", "[", "i", "]"}], "//", "N"}], "]"}]}], "]"}], " ", 
   RowBox[{"(*", 
    RowBox[{
    "The", " ", "last", " ", "entry", " ", "in", " ", "the", " ", "matrix", 
