Cell[BoxData[{
 RowBox[{"X", ":=", 
  RowBox[{"RandomReal", "[", "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"h", "[", "x_", "]"}], ":=", 
  RowBox[{"Piecewise", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"0", ",", 
       RowBox[{"0", "\[LessEqual]", "x", "\[LessEqual]", " ", "0.03125"}]}], 
      "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"1", ",", 
       RowBox[{"0.03125", "<", "x", "\[LessEqual]", " ", "0.1875"}]}], "}"}], 
     ",", 
     RowBox[{"{", 
      RowBox[{"2", ",", 
       RowBox[{"0.1875", "<", "x", "\[LessEqual]", "0.5"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"3", ",", 
       RowBox[{"0.5", "<", "x", "\[LessEqual]", "0.8125"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"4", ",", 
       RowBox[{"0.8125", "<", "x", "\[LessEqual]", "0.96875"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"5", ",", 
       RowBox[{"0.96875", "<", "x", "\[LessEqual]", "1"}]}], "}"}]}], "}"}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"n", "=", "100000"}], ";", 
  RowBox[{"NumGenerations", "=", "10"}], ";", 
  RowBox[{"size", "=", "0"}], ";", 
  RowBox[{"count1", "=", "0"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Do", "[", " ", 
   RowBox[{
    RowBox[{
     RowBox[{"x", "=", "1"}], ";", "\[IndentingNewLine]", "  ", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", "       ", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"x", ">", "0"}], ",", 
         RowBox[{
          RowBox[{"a", "=", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"h", "[", "X", "]"}], ",", 
             RowBox[{"{", "x", "}"}]}], "]"}]}], ";", 
          RowBox[{"x", "=", 
           RowBox[{"Apply", "[", 
            RowBox[{"Plus", ",", "a"}], "]"}]}]}]}], "]"}], ",", 
       "\[IndentingNewLine]", "      ", 
       RowBox[{"{", "NumGenerations", "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", "  ", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"x", "\[Equal]", "0"}], ",", 
       RowBox[{"count1", "=", 
        RowBox[{"count1", "+", "1"}]}], ",", 
       RowBox[{"size", "=", 
        RowBox[{"size", "+", "x"}]}]}], " ", "]"}]}], ",", 
    "\[IndentingNewLine]", " ", 
    RowBox[{"{", "n", "}"}]}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Print", "[", 
   RowBox[{"\"\<frequency(extinction by 10-th generation) = \>\"", ",", 
    RowBox[{
     RowBox[{"count1", "/", "n"}], "//", "N"}]}], "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
