  RowBox[{"RandomReal", "[", "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"g", "[", "x_", "]"}], ":=", 
  RowBox[{"Piecewise", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"0", ",", 
       RowBox[{"0", "\[LessEqual]", "x", "\[LessEqual]", " ", 
        RowBox[{"7", "/", "12"}]}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"1", ",", 
       RowBox[{
        RowBox[{"7", "/", "12"}], "<", "x", "\[LessEqual]", " ", 
        RowBox[{"11", "/", "12"}]}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"2", ",", 
       RowBox[{
        RowBox[{"11", "/", "12"}], "<", "x", "\[LessEqual]", " ", "1"}]}], 
      "}"}]}], "}"}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"n", "=", "100000"}], ";", 
  RowBox[{"NumGenerations", "=", "7"}], ";", 
  RowBox[{"size", "=", "0"}], ";", 
  RowBox[{"count1", "=", "0"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Do", "[", " ", 
   RowBox[{
    RowBox[{
     RowBox[{"x", "=", "1"}], ";", "\[IndentingNewLine]", "  ", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", "       ", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"x", ">", "0"}], ",", 
         RowBox[{
          RowBox[{"a", "=", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"g", "[", "X", "]"}], ",", 
             RowBox[{"{", "x", "}"}]}], "]"}]}], ";", 
          RowBox[{"x", "=", 
           RowBox[{"Apply", "[", 
            RowBox[{"Plus", ",", "a"}], "]"}]}]}]}], "]"}], ",", 
       "\[IndentingNewLine]", "      ", 
       RowBox[{"{", "NumGenerations", "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", "  ", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"x", "\[Equal]", "0"}], ",", 
       RowBox[{"count1", "=", 
        RowBox[{"count1", "+", "1"}]}], ",", 
       RowBox[{"size", "=", 
        RowBox[{"size", "+", "x"}]}]}], " ", "]"}]}], ",", 
    "\[IndentingNewLine]", " ", 
    RowBox[{"{", "n", "}"}]}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Print", "[", 
